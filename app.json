{
  "expo": {
    "name": "Place Vault",
    "slug": "PlaceVault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/mainIcon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icons/mainIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0275d8"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/mainIcon.png",
        "backgroundColor": "#0275d8"
      },
      "config": {
        "googleMaps": {
          "apiKey": "GOOGLE_API_KEY"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.zthyphon.PlaceVault"
    },
    "web": {
      "favicon": "./assets/icons/mainIcon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "85eac45f-3462-4400-b65a-9d7533a19fc5"
      }
    }
  }
}
